<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="at.htlleonding.demo.addressbook.controller.AddressBookController">   <children>
    <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
        <children>
            <TextField fx:id="search_text_field" alignment="CENTER" prefHeight="32.0" prefWidth="196.0">
                <HBox.margin>
                    <Insets left="20.0" />
                </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#onBtnSearchAction" prefHeight="30.0" prefWidth="58.0" text="search">
                <HBox.margin>
                    <Insets left="10.0" />
                </HBox.margin>
            </Button>
        </children>
        <VBox.margin>
            <Insets />
        </VBox.margin>
    </HBox>
    <HBox prefHeight="289.0" prefWidth="600.0" spacing="10.0">
        <children>
            <VBox prefHeight="244.0" prefWidth="267.0">
                <children>
                    <ListView fx:id="listview_lbl" prefHeight="245.0" prefWidth="247.0">
                        <VBox.margin>
                            <Insets left="20.0" />
                        </VBox.margin>
                    </ListView>
                    <HBox alignment="CENTER_LEFT" prefHeight="37.0" prefWidth="267.0">
                        <VBox.margin>
                            <Insets left="20.0" top="10.0" />
                        </VBox.margin>
                        <children>
                            <Label prefHeight="28.0" prefWidth="139.0" text="Number of Contacts: ">
                                <font>
                                    <Font size="13.0" />
                                </font>
                            </Label>
                            <Label fx:id="number_of_contacts_lbl" prefHeight="17.0" prefWidth="38.0" />
                        </children>
                    </HBox>
                </children>
            </VBox>
            <VBox prefHeight="246.0" prefWidth="309.0">
                <HBox.margin>
                    <Insets bottom="50.0" />
                </HBox.margin>
                <children>
                    <GridPane>
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="90.0" />
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="219.0" minWidth="10.0" prefWidth="219.0" />
                        </columnConstraints>
                        <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        </rowConstraints>
                        <VBox.margin>
                            <Insets bottom="30.0" />
                        </VBox.margin>
                        <children>
                            <Label prefHeight="28.0" prefWidth="139.0" text="ID:">
                                <font>
                                    <Font size="13.0" />
                                </font>
                            </Label>
                            <Label prefHeight="28.0" prefWidth="139.0" text="Name:" GridPane.rowIndex="1">
                                <font>
                                    <Font size="13.0" />
                                </font>
                            </Label>
                            <Label prefHeight="28.0" prefWidth="139.0" text="Phone:" GridPane.rowIndex="2">
                                <font>
                                    <Font size="13.0" />
                                </font>
                            </Label>
                            <Label prefHeight="28.0" prefWidth="139.0" text="E-Mail" GridPane.rowIndex="3">
                                <font>
                                    <Font size="13.0" />
                                </font>
                            </Label>
                            <TextField fx:id="name_text_field" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                            <TextField fx:id="id_text_field" GridPane.columnIndex="1" />
                            <TextField fx:id="email_text_field" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                            <TextField fx:id="phone_text_field" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        </children>
                    </GridPane>
                    <GridPane alignment="CENTER">
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        </columnConstraints>
                        <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        </rowConstraints>
                        <children>
                            <Button fx:id="delete_btn" mnemonicParsing="false" onAction="#onBtnDeleteAction" prefHeight="25.0" prefWidth="94.0" text="Delete" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                            <Button fx:id="new_btn" mnemonicParsing="false" onAction="#onBtnNewAction" prefHeight="25.0" prefWidth="94.0" text="New" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                            <Button fx:id="save_btn" mnemonicParsing="false" onAction="#onBtnSaveAction" prefHeight="25.0" prefWidth="94.0" text="Save" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                        </children>
                    </GridPane>
                </children>
            </VBox>
        </children>
    </HBox>
</children>
</VBox>
